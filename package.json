{
  "workspaces": [
    "App/*",
    "Landing/*"
  ],
  "turbo": {
    "baseBranch": "origin/main",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": [".next/**"]
      },
      "test": {
        "dependsOn": ["^build"],
        "outputs": []
      },
        "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  },
  "devDependencies": {
    "turbo": "^1.0.26"
  }
}
